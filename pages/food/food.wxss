/* pages/food/food.wxss */
page {
  background: #f2f2f2;
}
.search input {
  width:100%;
  background: #fff;
  border-radius: 10rpx;
  height:59rpx;
  line-height: 59rpx;
  text-align: center;
  font-size: 27rpx;
  color: #8d8d92;
  margin-top: 19rpx;
}
.title {
  width:100%;
  height:160rpx;
  text-align: center;
  line-height: 160rpx;
}
.title image {
  width:368rpx;
  height:80rpx;
  vertical-align: middle;
}
.foodList navigator {
  width:100%;
  background: #fff;
  border-radius: 20rpx;
  height:397rpx;
  overflow: hidden;
  margin-bottom: 25rpx;
}
.foodList image {
  width:100%;
  height:288rpx;
}
.food {
  padding: 10rpx 22rpx 0 22rpx;
}

.food01 {font-size: 32rpx; color: #1c1c1c;}
.food02 {float:right;color: #85888f;font-size: 22rpx;
  background-repeat: no-repeat;
  background-size: 24rpx 25rpx;
  background-position: 0 5rpx;
  padding-left: 30rpx;
}
.food03 {display: block;color: #a9aaac;font-size: 22rpx;margin-top: 10rpx;}